module cmp8 (
        input a[8],
        input b[8],
        input alufn[6],
        output cmp[8]
    ) {

    always {
        cmp = 0;

        case (alufn[2:1]) {
            b01:
                cmp[0] = (a == b);

            b10:
                cmp[0] = (a < b);

            b11:
                cmp[0] = (a <= b);
        }
    }
}

module cmp8 (
        input a[8],
        input b[8],
        input alufn[6],
        output cmp[8]
    ) {

    always {
        cmp[7:1] = 0;

        case (alufn[2:1]) {
            01:
                cmp[0] = (a == b);

            10:
                cmp[0] = (a < b);

            11:
                cmp[0] = (a <= b);    
        }
    }
}

module bool8 (
        input a[8],
        input b[8],
        input alufn[6],
        output bool[8]
    ) {
    
    always {
        bool = 0;

        case (alufn[3:0]) {
            b1000:
                bool = a & b;
                
            b1110:
                bool = a | b;
                
            b0110:
                bool = a ^ b;
                
            b1010:
                bool = a;
             }
        }
    }

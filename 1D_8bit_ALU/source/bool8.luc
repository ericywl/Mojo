module bool8 (
    input a[8],
    input b[8],
    input alufn[6],
    output s[8]
  ) {
  
  always {
    s = 0;
    case (alufn[3:0]){
      1000:
        s = a & b;
        
      1110:
        s = a | b;
        
      0110:
        s = a ^ b;
        
      1010:
        s = a;
       }
    }
  }
